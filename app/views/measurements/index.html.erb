  
<h1> MEASUREMENTS </h1>

<% @measurements.each do |measurement| %>
  <div class="measurement">
    <%= measurement.id %>
    <%= measurement.created_at.strftime("%m/%d/%Y %Hh-%M") %>
    <%= image_tag measurement.photo.url(:small) %>
    <%= image_tag measurement.processed_photo.url(:small) %>
  </div>
<% end %>